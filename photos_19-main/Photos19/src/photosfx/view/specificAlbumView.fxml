<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.text.Font?>

<TitledPane fx:id="titlePane" alignment="CENTER"
	animated="false" collapsible="false" contentDisplay="CENTER"
	prefHeight="444.0" prefWidth="620.0" text="Photos in &quot;&quot;"
   xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photosfx.controllers.specificAlbumViewController">
   <content>
      <AnchorPane minHeight="0.0" minWidth="0.0"
               pickOnBounds="false" prefHeight="611.0" prefWidth="443.0">
         <children>
            <ListView fx:id="photosList" layoutX="14.0" layoutY="23.0" prefHeight="277.0" prefWidth="242.0" />
            <Button fx:id="next" layoutX="515.0" layoutY="105.0" mnemonicParsing="false" onAction="#next" prefHeight="26.0" prefWidth="64.0" text="Next" />
            <Button fx:id="prev" layoutX="268.0" layoutY="105.0" mnemonicParsing="false" onAction="#prev" text="Previous" />
            <Button fx:id="back" layoutX="541.0" layoutY="360.0" mnemonicParsing="false" onAction="#back" text="Back" />
            <Button fx:id="delete" layoutX="74.0" layoutY="313.0" mnemonicParsing="false" onAction="#delete" text="Delete " />
            <Button fx:id="quit" layoutX="14.0" layoutY="360.0" mnemonicParsing="false" onAction="#quit" text="Quit" />
            <Button fx:id="copy" layoutX="139.0" layoutY="313.0" mnemonicParsing="false" onAction="#copy" prefHeight="26.0" prefWidth="56.0" text="Copy" />
            <Button fx:id="move" layoutX="199.0" layoutY="313.0" mnemonicParsing="false" onAction="#move" prefHeight="26.0" prefWidth="56.0" text="Move" />
            <Button fx:id="add" layoutX="14.0" layoutY="313.0" mnemonicParsing="false" onAction="#add" prefHeight="26.0" prefWidth="56.0" text="Add " />
            <ImageView fx:id="imageShow" fitHeight="157.0" fitWidth="166.0" layoutX="341.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true" />
            <ListView fx:id="tagsList" layoutX="331.0" layoutY="280.0" prefHeight="59.0" prefWidth="160.0" />
            <TextField fx:id="captions" layoutX="331.0" layoutY="208.0" promptText="None" />
            <TextField fx:id="dateField" layoutX="331.0" layoutY="243.0" prefHeight="26.0" prefWidth="242.0" />
            <Button fx:id="addTag" layoutX="507.0" layoutY="280.0" mnemonicParsing="false" onAction="#addTag" prefHeight="26.0" prefWidth="64.0" text="Add" />
            <Button layoutX="507.0" layoutY="208.0" mnemonicParsing="false" onAction="#edit" prefHeight="26.0" prefWidth="64.0" text="Edit" />
            <Button fx:id="delTag" layoutX="507.0" layoutY="313.0" mnemonicParsing="false" onAction="#delTag" prefHeight="26.0" prefWidth="64.0" text="Delete" />
            <Label layoutX="277.0" layoutY="213.0" text="Caption:" />
            <Label layoutX="294.0" layoutY="248.0" text="Date:" />
            <Label layoutX="293.0" layoutY="285.0" text="Tags:" />
         </children>
        </AnchorPane>
    </content>
    <font>
        <Font name="Arial" size="17.0" />
    </font>
</TitledPane>
