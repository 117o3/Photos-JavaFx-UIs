<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photosfx.controllers.loginController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <TextField fx:id="username" layoutX="41.0" layoutY="87.0" promptText="username"/>
      <Label alignment="CENTER" layoutX="65.0" layoutY="29.0" prefHeight="26.0" prefWidth="159.0" text="Login" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="enter" layoutX="209.0" layoutY="85.0" mnemonicParsing="false" onAction="#enter" text="Enter" />
      <Label layoutX="79.0" layoutY="150.0" text="213 Photos Assignment" />
   </children>
</AnchorPane>
